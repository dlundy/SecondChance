<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(dog_application_question) -%>">
  <span class='title'>
    <%= dog_application_question.question_text %>
    <span class="preview">&nbsp;</span>
  </span>
  <span class='actions'>
    <%= link_to refinery_icon_tag("application_go.png"), dog_application_question_path(dog_application_question),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    <%= link_to refinery_icon_tag("application_edit.png"), edit_admin_dog_application_question_path(dog_application_question),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), admin_dog_application_question_path(dog_application_question),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'shared.admin.delete', :title => dog_application_question.question_text),
        :method => :delete %>
  </span>
</li>
