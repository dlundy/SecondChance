<% content_for :body_content_title do %>
  <%= @dog_application_question.question_text %>
<% end %>

<% content_for :body_content_left do %>
  <section>
    <h1>Question Text</h1>
    <p>
      <%=raw @dog_application_question.question_text %>
    </p>
  </section>
<% end %>

<% content_for :body_content_right do %>
  <aside>
    <h2><%= t('.other') %></h2>
    <ul id="dog_application_questions">
      <% @dog_application_questions.each do |dog_application_question| %>
        <li>
          <%= link_to dog_application_question.question_text, dog_application_question_path(dog_application_question) %>
        </li>
      <% end %>
    </ul>
  </aside>
<% end %>

<%= render :partial => "/shared/content_page" %>
