<% content_for :body_content_title do %>
  <%= @member.email %>
<% end %>

<% content_for :body_content_left do %>
  <section>
    <%= link_to 'Update Volunteer Profile', edit_member_path(@member) %>
  </section>
  <section>
    <%= image_tag(@member.profile_image.url) if @member.profile_image %>
  </section>
  <section>
    <h1>Email</h1>
    <p>
      <%=raw @member.email %>
    </p>
  </section>
  <section>
    <h1>First Name</h1>
    <p>
      <%=raw @member.first_name %>
    </p>
  </section>
  <section>
    <h1>Last Name</h1>
    <p>
      <%=raw @member.last_name %>
    </p>
  </section>
  <section>
    <h1>Address 1</h1>
    <p>
      <%=raw @member.address_1 %>
    </p>
  </section>
  <section>
    <h1>Address 2</h1>
    <p>
      <%=raw @member.address_2 %>
    </p>
  </section>
  <section>
    <h1>City</h1>
    <p>
      <%=raw @member.city %>
    </p>
  </section>
  <section>
    <h1>State</h1>
    <p>
      <%=raw @member.state %>
    </p>
  </section>
  <section>
    <h1>Zipcode</h1>
    <p>
      <%=raw @member.zipcode %>
    </p>
  </section>
  <section>
    <h1>Phone</h1>
    <p>
      <%=raw @member.phone %>
    </p>
  </section>
  <section>
    <h1>Description</h1>
    <p>
      <%=raw @member.description %>
    </p>
  </section>
<% end %>


<%= render :partial => "/shared/content_page" %>
