- content_for :body_content_left do 
  #members_index
    %h3 Volunteer Index
    %table
      %tr
        %th First Name
        %th Last Name
        %th Email
        %th Phone
      - @members.each do |member|
        %tr{:class => cycle('odd', 'even')}
          %td= member.first_name.to_s
          %td= member.last_name.to_s
          %td
            = member.email unless member.private_email 
          %td
            = member.phone.to_s unless member.private_phone
    %section
      = link_to 'Back', member_path(current_member)
    %section
      = link_to 'Home Page', root_path
= render :partial => "/shared/content_page"
