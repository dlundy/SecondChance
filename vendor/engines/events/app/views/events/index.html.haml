- content_for :body_content_left do
  %ul{ :id => "event_list" }
    - @events.each do |event|
      %li
        = image_tag(event.main_image.thumb("100x150").url) if event.main_image.present?
        = link_to raw(event_inline_listing(event)), event_path(event)

%div 
  = render :partial => "/shared/content_page"
